{
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:node/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "es2021": true
  },
  "rules": {
    "node/no-unpublished-import": "off",
    "node/no-deprecated-api": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "curly": [
      "error",
      "multi",
      "consistent"
    ],
    "comma-dangle": [
      "error",
      "never"
    ]
  }
}
