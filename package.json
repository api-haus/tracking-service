{
  "name": "tracker",
  "version": "0.0.0",
  "description": "Event Tracker and Stats services.",
  "main": "src/index.mjs",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "TS_NODE_FILES=true JAEGER_DISABLED=true ava --timeout=10m --verbose",
    "start": "node src/index.mjs",
    "build": "ttsc",
    "profile": "MULTICORE=true ARTILLERY_WORKERS=4 artillery run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurihq/tracker.git"
  },
  "author": "Yuri Chirkov <mail@api.haus>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/yurihq/tracker/issues"
  },
  "homepage": "https://github.com/yurihq/tracker#readme",
  "dependencies": {
    "@sentry/node": "^6.3.6",
    "async-lock": "^1.3.0",
    "clickhouse": "^1.2.88888888",
    "clickhouse-sql": "^1.2.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "fast-json-stringify": "^2.7.3",
    "fastify": "^3.15.1",
    "fastify-cookie": "^5.3.1",
    "fastify-metrics": "^7.1.0",
    "fastify-plugin": "^3.0.0",
    "inversify": "^5.1.1",
    "jaeger-client": "^3.18.1",
    "kafkajs": "^1.15.0",
    "lru-cache": "^6.0.0",
    "ms": "^2.1.3",
    "opentracing": "^0.14.5",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "prom-client": "^13.1.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/eslint-parser": "^7.14.2",
    "@babel/plugin-syntax-top-level-await": "^7.12.13",
    "@babel/preset-env": "^7.14.2",
    "@types/async-lock": "^1.1.2",
    "@types/jaeger-client": "^3.18.0",
    "@types/kafkajs": "^1.9.0",
    "@types/lru-cache": "^5.1.0",
    "@types/pg": "^7.14.11",
    "@types/ramda": "^0.27.40",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "artillery": "1.6",
    "ava": "^3.15.0",
    "eslint": "^7.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-node": "^11.1.0",
    "got": "^11.8.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "require": [
      "tsconfig-paths/register"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "files": [
      "src/**/*.spec.ts"
    ]
  }
}
